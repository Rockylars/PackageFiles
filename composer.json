{
    "name": "rockylars/package-files",
    "description": "Simple test call to tell you what will go in a package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rockylars",
            "email": "rockylars@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rocky\\PackageFiles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rocky\\PackageFiles\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpunit/phpunit": "^9.6",
        "spaze/phpstan-disallowed-calls": "^3.1"
    }
}
